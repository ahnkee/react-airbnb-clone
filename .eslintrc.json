{
    "extends": ["eslint:recommended", "google", "prettier", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error",
      "prefer-const": "error"
    }
  }
